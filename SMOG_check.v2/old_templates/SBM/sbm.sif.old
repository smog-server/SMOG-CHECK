<!-- HAMILTONIAN SETTINGS -->
<settings>
	<!-- ENERGY/CONTACT GROUPS -->
<Groups>
														<!-- NUCLEIC ENERGY GROUPS -->
	<!-- Normalized -->
	<energyGroup residueType="nucleic" name="bb_n" interRelativeStrength="1" intraRelativeStrength="PARM_N_BB" normalize="1"/>
	<energyGroup residueType="nucleic" name="sc_n" interRelativeStrength="1" intraRelativeStrength="PARM_N_SC" normalize="1"/>
	<!-- Not-Normalized -->
	<energyGroup residueType="nucleic" name="pr_n" interRelativeStrength="1" normalize="0"/>
	<energyGroup residueType="nucleic" name="ip_n" interRelativeStrength="1" normalize="0"/>
	<energyGroup residueType="nucleic" name="r_n" interRelativeStrength="1" normalize="0"/>
														<!-- AMINO ENERGY GROUPS --> 
<!-- Normalized -->
	<energyGroup residueType="amino" name="bb_a" interRelativeStrength="1" intraRelativeStrength="PARM_P_BB" normalize="1"/>
	<energyGroup residueType="amino" name="sc_a" interRelativeStrength="1" intraRelativeStrength="PARM_P_SC" normalize="1"/>
	<!-- Not-Normalized -->
	<energyGroup residueType="amino" name="pr_a" interRelativeStrength="1" normalize="0"/>
	<energyGroup residueType="amino" name="ip_a" interRelativeStrength="1" normalize="0"/>
	<energyGroup residueType="amino" name="r_a" interRelativeStrength="1" normalize="0"/>

 <!-- Global Contacts Settings -->
	<contactGroup residueType="mixed" name="c" interRelativeStrength="PARM_C_D" intraRelativeStrength="1" normalize="1"/>
</Groups>
<!-- CONTACT MAP SETTINGS -->
<Contacts method="shadow" contactDistance="6" shadowRadius="1"/>


</settings>
